{
    "name": "kudobuzz/laravel-bigcommerce",
    "description": "Laravel bigcommerce package for kudobuzz users",
    "license": "MIT",
    "authors": [
        {
            "name": "kudobuzz"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.4",
        "bigcommerce/api": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kudobuzz\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
     "extra": {
         "laravel": {
             "providers": [
                 "Kudobuzz\\Bigcommerce\\BigcommerceServiceProvider"
             ],
             "aliases": {
                 "Bigcommerce": "Kudobuzz\\Bigcommerce\\Facades\\Bigcommerce"
             }
         }
    }
}
